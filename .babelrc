{
	"presets": [
		[ "env", {
			"targets": {
				"browsers": [
					"last 2 versions",
					"Safari >= 9",
					"iOS >= 9",
					"not ie <= 10",
					"> 1%"
				]
			}
		} ],
		"stage-2"
	],
	"plugins": [
		"transform-runtime",
		"transform-export-extensions",
		"add-module-exports",
		"syntax-jsx",
		"transform-react-jsx",
		"transform-react-display-name",
		"lodash",
		[
			"transform-imports",
			{
				"state/selectors": {
					"transform": "state/selectors/${member}",
					"kebabCase": true
				}
			}
		]
	]
}
